{
    "name": "mixasmix/autopart",
    "description": "Autopart",
    "type": "project",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "6.2.*",
        "doctrine/orm": "2.12.3",
        "symfony/validator": "6.2.*",
        "doctrine/common": "3.3.0",
        "symfony/console": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/yaml": "6.2.*",
        "doctrine/doctrine-migrations-bundle": "3.3.x-dev",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/flex": "2.2.*",
        "doctrine/doctrine-bundle": "2.7.0",
        "symfony/dotenv": "6.2.*",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/expression-language": "6.2.*",
        "zircote/swagger-php": "4.x-dev",
        "guzzlehttp/guzzle": "7.9.x-dev",
        "symfony/serializer": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "m.novokreshenov",
            "email": "mixasmix@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.48",
        "phpunit/phpunit": "11.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
